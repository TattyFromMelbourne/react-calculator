{
	"name": "react-calculator",
	"version": "0.0.1",
	"private": true,
	"dependencies": {
		"mathjs": "^6.0.3",
		"react": "^16.8.6",
		"react-dom": "^16.8.6",
		"react-scripts": "3.0.1"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test --watchAll",
		"test:staged": "CI=true react-scripts test --passWithNoTests",
		"eject": "react-scripts eject",
		"validate": "npm run format && npm run lint:fix && npm run test:staged",
		"format": "prettier --write ./src",
		"lint:fix": "eslint --fix ./src",
		"initial": "npm run install:husky && npm run install:husky",
		"install:husky": "husky install",
		"husky:pre-commit": "npx husky add .husky/pre-commit \"npm run lint-staged\"",
		"lint-staged": "lint-staged"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"homepage": "https://lensoftware.com/presentations/2018-02-19/",
	"devDependencies": {
		"@testing-library/jest-dom": "^5.14.1",
		"@testing-library/react": "^12.1.0",
		"@testing-library/user-event": "^13.2.1",
		"@typescript-eslint/eslint-plugin": "^4.0.0",
		"@typescript-eslint/parser": "^4.0.0",
		"babel-eslint": "^10.0.0",
		"eslint": "^7.5.0",
		"eslint-config-react-app": "^6.0.0",
		"eslint-plugin-flowtype": "^5.2.0",
		"eslint-plugin-import": "^2.22.0",
		"eslint-plugin-jest": "^24.4.0",
		"eslint-plugin\t-jsx-a11y": "^6.3.1",
		"eslint-plugin-react": "^7.20.3",
		"eslint-plugin-react-hooks": "^4.0.8",
		"eslint-plugin-testing-library": "^4.12.2",
		"husky": "^7.0.2",
		"lint-staged": "^11.1.2",
		"prettier": "^2.4.0"
	}
}
